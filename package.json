{
  "name": "@metaxia/scriptures-source-huggingface-hmcgovern-original-language-bibles-greek-stepbible-tagnt-tr",
  "version": "0.1.2",
  "description": "Textus Receptus (Greek) data for @metaxia/scriptures. Source: STEPBible TAGNT via HuggingFace.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "import": "./dist/register.js"
    },
    "./metadata": "./data/hf-hmcgovern-olb-greek-stepbible-tagnt-tr/metadata.json",
    "./data/*": "./data/*",
    "./cache/*": "./cache/*",
    "./books/*": {
      "types": "./dist/books/*.d.ts",
      "import": "./dist/books/*.js"
    }
  },
  "files": [
    "dist",
    "data",
    "cache"
  ],
  "scripts": {
    "build": "tsc",
    "import": "tsx scripts/import.ts",
    "test": "vitest run",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "bible",
    "greek",
    "textus-receptus",
    "scriptures",
    "new-testament",
    "stepbible"
  ],
  "author": "Benjamin Plunkert",
  "license": "MIT AND CC-BY-4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/metaxiamultimedia/scriptures-js-source-huggingface-hmcgovern-original-language-bibles-greek-stepbible-tagnt-tr"
  },
  "bugs": {
    "url": "https://github.com/metaxiamultimedia/scriptures-js-source-huggingface-hmcgovern-original-language-bibles-greek-stepbible-tagnt-tr/issues"
  },
  "homepage": "https://github.com/metaxiamultimedia/scriptures-js-source-huggingface-hmcgovern-original-language-bibles-greek-stepbible-tagnt-tr#readme",
  "peerDependencies": {
    "@metaxia/scriptures-core": "github:metaxiamultimedia/scriptures-js-core#v0.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@huggingface/hub": "^0.15.0",
    "@metaxia/scriptures-core": "github:metaxiamultimedia/scriptures-js-core#v0.1.2",
    "@types/node": "^25.0.6",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "apache-arrow": "^18.0.0",
    "eslint": "^9.39.2",
    "parquet-wasm": "^0.6.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.0"
  }
}
